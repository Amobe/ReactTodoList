{
  "name": "webpack-ex",
  "version": "0.0.1",
  "description": "practice",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --colors --content-base build --hot"
  },
  "author": "Lexamo",
  "license": "ISC",
  "jshintConfig": {
    "esversion": "6",
    "browser": true,
    "curly": true,
    "devel": true,
    "eqeqeq": true,
    "evil": true,
    "forin": true,
    "jquery": true,
    "noarg": true,
    "noempty": true,
    "strict": true,
    "undef": true,
    "wsh": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "eol-last": [
        0
      ],
      "no-mixed-requires": [
        0
      ],
      "no-underscore-dangle": [
        0
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "webpack": "^1.13.0"
  }
}
